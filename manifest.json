{
  "manifest_version": 2,
  "name": "Mouse Killer",
"description": "you can search on google just on the keyboard! you need not use the tiny tiny stressive mouse!",
  "options_page": "options.html",
  "background": {
    "scripts": [ "./js/background.js" ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./js/content.js","./lib/jquery-3.3.1.min.js"
      ]
    }],
  "permissions": [
    "tabs"
  ],
  "version": "0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "./icons/icon16.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  }
}
